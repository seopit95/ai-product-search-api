    "@qdrant/js-client-rest": "^1.16.2",
    "dotenv": "^17.2.3",
    "openai": "^6.16.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@types/node": "^24.6.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
